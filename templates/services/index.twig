{% extends "layout/service.twig" %}
{% block content %}


<body>
    <div id="webpage" class="page-wrapper">
       
        <div class="global-styles w-embed"></div>
        <main class="main">
            <section class="section wf-section">
                <div class="hero-with-vertical-lines-2">
                    <div class="hero-vertical-content">
                        <p data-w-id="4ce61da8-3a47-04be-7240-9f17170871d0" class="uppercase-title-vertical">We provide
                        </p>
                        <h1 data-w-id="4ce61da8-3a47-04be-7240-9f17170871d2" class="heading in-hero-vertical">Property
                            construction &amp; renovation Solutions</h1>
                    </div>
                    <div class="absolute-block">
                        <div data-w-id="4ce61da8-3a47-04be-7240-9f17170871d5" class="hero-background-color"></div>
                        <div class="_3d-block">
                            <div class="image-animation-trigger _3d-for-image">
                                <div class="animation-color-bg"></div><img
                                    src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6260157385d08d520bc7f912_pexels-rodolfo-quir%C3%B3s-2219024.jpg"
                                    alt="Construction and renovation webflow template sample image" sizes="100vw"
                                    srcset="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6260157385d08d520bc7f912_pexels-rodolfo-quir%C3%B3s-2219024-p-500.jpeg 500w, https://assets.website-files.com/625295532800cfe2dd8d3dd2/6260157385d08d520bc7f912_pexels-rodolfo-quir%C3%B3s-2219024-p-800.jpeg 800w, https://assets.website-files.com/625295532800cfe2dd8d3dd2/6260157385d08d520bc7f912_pexels-rodolfo-quir%C3%B3s-2219024-p-1080.jpeg 1080w, https://assets.website-files.com/625295532800cfe2dd8d3dd2/6260157385d08d520bc7f912_pexels-rodolfo-quir%C3%B3s-2219024.jpg 1920w"
                                    class="cover-image" />
                            </div>
                        </div>
                    </div>
                    <div data-w-id="4ce61da8-3a47-04be-7240-9f17170871da" class="grid-for-vertical-lines">
                        <div class="vertical-line with-bg-opacity-15"></div>
                        <div class="vertical-line with-bg-opacity-25"></div>
                        <div class="vertical-line with-bg-opacity-5"></div>
                        <div class="vertical-line"></div>
                        <div class="vertical-line"></div>
                        <div class="vertical-line with-bg-opacity-5"></div>
                        <div class="vertical-line with-bg-opacity-5 tab-is-hidden"></div>
                        <div class="vertical-line with-bg-opacity-15 tab-is-hidden"></div>
                    </div>
                </div>
                <div class="w-layout-grid grid-for-button">
                    <div id="w-node-_3bd9fc55-29eb-5d0c-e889-b67a513455dd-170871cd" class="grid-button-item"><a
                            href="#start" class="explore-link-button w-inline-block">
                            <p class="explore-text">Explore</p><img
                                src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6253198640cd9ac45afbec8b_arrow_downward_white_24dp.svg"
                                loading="lazy" alt="" class="explore-icon" />
                            <div class="explore-background"></div>
                        </a></div>
                </div>
            </section>
            <section id="start" class="section wf-section">
                <div class="solution-cms-3 w-dyn-list">
                    <div role="list" class="solution-cms-3-list w-dyn-items">
                    {% set  services = craft.entries.section('services').all() %}
                    {% for service in services %}
                    {% set photo = service.serviceImage.one() %}
                                    
                        <div role="listitem" class="solution-cms-3-item w-dyn-item">
                            <div class="solution-cms-3-content">
                                <div class="moving-link in-solutions-3"><img alt="" loading="lazy"
                                        src="https://assets.website-files.com/6252f3c395e36c6da2794a2c/6267ab812c0e7325a623d543_construction_FILL0_wght400_GRAD0_opsz48_white.svg"
                                        class="moving-icon is-larger" />
                                    <div class="moving-link-circle"></div>
                                </div>
                                <h2 data-w-id="a5f2a85c-d090-fa5c-295b-6564485b3f08" style="opacity:0"
                                    class="solution-cms-2-title">{{service.title}}</h2>
                                <p data-w-id="45fd1168-2d7d-b0bb-a702-b290824e449b" style="opacity:0"
                                    class="solution-cms-3-desciption">{{service.serviceDescription}}</p><a href="/services/{{service.title}}"
                                    class="solution-button w-inline-block">
                                    <p>Learn More</p><img
                                        src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6252f1730653975c82d4a73c_east_white_24dp.svg"
                                        loading="lazy" alt="" class="button-icon" />
                                </a>
                            </div>
                            <div id="w-node-_45fd1168-2d7d-b0bb-a702-b290824e44a0-f5f9ca47"
                                class="solution-cms-3-image">
                                <div class="_3d-block">
                                    <div class="image-animation-trigger _3d-for-image">
                                        <div class="animation-color-bg"></div><img
                                            src="{{photo.url}}"
                                            loading="lazy" alt="Construction"
                                            sizes="(max-width: 479px) 78vw, (max-width: 767px) 60vw, (max-width: 991px) 28vw, 31vw"
                                            {# srcset="https://assets.website-files.com/6252f3c395e36c6da2794a2c/626701d4b43dd05c344c6ca7_pexels-tima-miroshnichenko-6615028-p-500.jpeg 500w, https://assets.website-files.com/6252f3c395e36c6da2794a2c/626701d4b43dd05c344c6ca7_pexels-tima-miroshnichenko-6615028-p-800.jpeg 800w, https://assets.website-files.com/6252f3c395e36c6da2794a2c/626701d4b43dd05c344c6ca7_pexels-tima-miroshnichenko-6615028-p-1080.jpeg 1080w, https://assets.website-files.com/6252f3c395e36c6da2794a2c/626701d4b43dd05c344c6ca7_pexels-tima-miroshnichenko-6615028.jpg 1920w" #}
                                            class="cover-image" />
                                    </div>
                                </div>
                            </div>
                        
                        </div>
                             {% endfor %}
                       
                    </div>
                </div>
            </section>
         
            
        </main>
    </div>
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=625295532800cfe2dd8d3dd2"
        type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
        crossorigin="anonymous"></script>
    <script src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/js/webflow.097fc263b.js"
        type="text/javascript"></script>
    <!--[if lte IE 9]><script src="//cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif]-->
</body>

{% endblock %}