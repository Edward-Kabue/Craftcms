{% extends "layout/_projects.twig" %}
{% block content %}

{% set projects = craft.entries.section('projects').all() %}
<body >
        <div class="preloader">
                <div class="preloader-inner">
                    <div class="preloader-brand">
                        {% set logo = header.logo.one() %}
                            {% if logo %}
                                <img src="{{ logo.url }}" loading="lazy" alt="" class="preloader-logo" alt="logo" />
                            {% endif %}
                    </div>
                    <div class="preloader-line"></div>
                    <div class="preloader-content">
                        <div class="preloader-title-overflow">
                            <p class="preloader-content-title is-1st">{{preloader.preloaderTitle}}</p>
                        </div>
                        {# <div class="preloader-title-overflow">
                            <p class="preloader-content-title is-2nd">construction </p>
                        </div> #}
                        <div class="preloader-title-overflow">
                            <p class="preloader-content-title is-3rd">{{preloader.preloaderSubtitle}}</p>
                        </div>
                    </div>
                </div>
                <p class="preloader-loading">Loading</p>
                <div class="preloader-background"></div>
        </div>
    <div id="webpage" class="page-wrapper">

            <div class="global-styles w-embed"></div>
        
            <div class="second-navbar"><a href="#" class="aside-contact-button w-inline-block">
            <img src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6258131bac44e22abdd9e8ce_email_white_24dp.svg"
                        loading="lazy" alt="" class="aside-icon" /></a>
                        <a href="#start" class="scroll-to-button w-inline-block">
                            <div class="aside-button-line"></div><img
                        src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/625319874a18755529409557_expand_more_white_24dp.svg"
                        loading="lazy" alt="" class="icon-scroll-to" />
                </a>
                <a href="#start" class="right-menu-button is-goes-up w-inline-block"><img
                        src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/62587db8e356a3457738948a_expand_less_white_24dp.svg"
                        loading="lazy" alt="" class="goes-up-icon" /></a>
                        </div>
            <main class="main">
                <section class="section wf-section">
                    <div class="hero-with-vertical-lines-2">
                        <div class="hero-vertical-content">
                            <p data-w-id="4ce61da8-3a47-04be-7240-9f17170871d0" class="uppercase-title-vertical">Projects
                            </p>
                            <h1 data-w-id="4ce61da8-3a47-04be-7240-9f17170871d2" class="heading in-hero-vertical">A LEADING
                                CONSTRUCTION AND RENOVATION COMPANY</h1>
                        </div>
                        <div class="absolute-block">
                            <div data-w-id="4ce61da8-3a47-04be-7240-9f17170871d5" class="hero-background-color"></div>
                            <div class="_3d-block">
                                <div class="image-animation-trigger _3d-for-image">
                                    <div class="animation-color-bg"></div><img
                                        src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6260157385d08d520bc7f912_pexels-rodolfo-quir%C3%B3s-2219024.jpg"
                                        alt="Construction and renovation webflow template sample image" sizes="100vw"
                                        srcset="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6260157385d08d520bc7f912_pexels-rodolfo-quir%C3%B3s-2219024-p-500.jpeg 500w, https://assets.website-files.com/625295532800cfe2dd8d3dd2/6260157385d08d520bc7f912_pexels-rodolfo-quir%C3%B3s-2219024-p-800.jpeg 800w, https://assets.website-files.com/625295532800cfe2dd8d3dd2/6260157385d08d520bc7f912_pexels-rodolfo-quir%C3%B3s-2219024-p-1080.jpeg 1080w, https://assets.website-files.com/625295532800cfe2dd8d3dd2/6260157385d08d520bc7f912_pexels-rodolfo-quir%C3%B3s-2219024.jpg 1920w"
                                        class="cover-image" />
                                </div>
                            </div>
                        </div>
                        <div data-w-id="4ce61da8-3a47-04be-7240-9f17170871da" class="grid-for-vertical-lines">
                            <div class="vertical-line with-bg-opacity-15"></div>
                            <div class="vertical-line with-bg-opacity-25"></div>
                            <div class="vertical-line with-bg-opacity-5"></div>
                            <div class="vertical-line"></div>
                            <div class="vertical-line"></div>
                            <div class="vertical-line with-bg-opacity-5"></div>
                            <div class="vertical-line with-bg-opacity-5 tab-is-hidden"></div>
                            <div class="vertical-line with-bg-opacity-15 tab-is-hidden"></div>
                        </div>
                    </div>
                    <div class="w-layout-grid grid-for-button">
                        <div id="w-node-_3bd9fc55-29eb-5d0c-e889-b67a513455dd-170871cd" class="grid-button-item"><a
                                href="#start" class="explore-link-button w-inline-block">
                                <p class="explore-text">Explore</p><img
                                    src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6253198640cd9ac45afbec8b_arrow_downward_white_24dp.svg"
                                    loading="lazy" alt="" class="explore-icon" />
                                <div class="explore-background"></div>
                            </a></div>
                    </div>
                </section>
                <section id="start" class="section wf-section">
                
                    <div class="container">
                        <div class="padding-inner">
                            <div class="projects-with-lines">
                                <div class="projects-cms w-dyn-list">
                                    <div role="list" class="project-cms-grid w-dyn-items">
                                    {% for project in projects %}
                                    {% set projectPhoto = project.coverimage.one() %}
                                        <div role="listitem" class="project-cms-item w-dyn-item"><a
                                                href="projects/{{ project.slug }}"
                                                class="project-link-block w-inline-block">
                                                <div class="project-link">
                                                    <div class="project-name-overlap">
                                                        <p class="project-name">
                                                    {{ project.projectName }}
                                                        </p>
                                                    </div><img
                                                        src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6256816300672a60b4c5c6e3_blue-arrow-right.svg"
                                                        loading="lazy" alt="" class="project-icon" />
                                                    <div class="project-bg-1"></div><img
                                                        src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6252f1730653975c82d4a73c_east_white_24dp.svg"
                                                        loading="lazy" alt="" class="project-icon-for-hover" />
                                                    <div class="project-bg-2"></div>
                                                    <div class="project-link-lines">
                                                        <div class="project-line-1"></div>
                                                        <div class="project-line-2"></div>
                                                    </div>
                                                </div>
                                                <div data-w-id="95166b7e-0066-a9aa-0f1a-c0d6558cca62" style="opacity:0"
                                                    class="project-gradient-bg"></div>
                                                <div class="_3d-block">
                                                    <div class="image-animation-trigger _3d-for-image">
                                                        <div class="animation-color-bg is-white-color"></div><img
                                                            src="{{projectPhoto.Url}}"
                                                            loading="lazy" alt="{{project.projectName}}" sizes="45vw"
                                                            {# srcset="https://assets.website-files.com/6252f3c395e36c6da2794a2c/62545e9d59e6100b9689423f_pexels-max-vakhtbovych-7546213-p-800.jpeg 800w, https://assets.website-files.com/6252f3c395e36c6da2794a2c/62545e9d59e6100b9689423f_pexels-max-vakhtbovych-7546213-p-1080.jpeg 1080w, https://assets.website-files.com/6252f3c395e36c6da2794a2c/62545e9d59e6100b9689423f_pexels-max-vakhtbovych-7546213-p-1600.jpeg 1600w, https://assets.website-files.com/6252f3c395e36c6da2794a2c/62545e9d59e6100b9689423f_pexels-max-vakhtbovych-7546213.jpg 1920w" #}
                                                            class="cover-image" />
                                                    </div>
                                                </div>
                                            </a>
                                        </div>
                                    {% endfor %}
                                    
                                    </div>
                                </div>
                                <div class="blue-lines---middle">
                                    <div class="blue-line-1"></div>
                                    <div class="blue-line-2"></div>
                                </div>
                                <div class="blue-lines---middle-right">
                                    <div class="blue-line-1"></div>
                                    <div class="blue-line-2"></div>
                                </div>
                                <div class="blue-lines---middle-left">
                                    <div class="blue-line-1"></div>
                                    <div class="blue-line-2"></div>
                                </div>
                                <div class="blue-lines---corner-left-2">
                                    <div class="blue-line-1"></div>
                                    <div class="blue-line-2"></div>
                                </div>
                                <div class="blue-lines---corner-left">
                                    <div class="blue-line-1"></div>
                                    <div class="blue-line-2"></div>
                                </div>
                                <div class="blue-lines---corner-right">
                                    <div class="blue-line-1"></div>
                                    <div class="blue-line-2"></div>
                                </div>
                                <div class="blue-lines---corner-right-2">
                                    <div class="blue-line-1"></div>
                                    <div class="blue-line-2"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
                <div class="pattern-divider"></div>

                <section class="section cta-content-1 wf-section">
                    <div class="container is-cta-container">
                        <div class="w-layout-grid content-grid-4">
                            <div class="content-padding-2">
                                <div class="content-flex-1">
                                    <p data-w-id="c4442ee1-5837-2ede-b004-647ecf03e420" class="uppercase-title-border">
                                        Contact</p>
                                    <h2 data-w-id="c4442ee1-5837-2ede-b004-647ecf03e422" class="heading is-cta-title-1">
                                        Reach out today and start this amazing journey with us</h2>
                                    <div class="two-buttons-1">
                                        <div class="button-overlap-6"><a href="/contact-1"
                                                class="main-button w-inline-block">
                                                <div class="button-inner">
                                                    <p class="button-title">Contact</p><img
                                                        src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6252f1730653975c82d4a73c_east_white_24dp.svg"
                                                        loading="lazy" alt="" class="button-icon" />
                                                </div>
                                                <div class="button-pattern"></div>
                                            </a></div><a data-w-id="c4442ee1-5837-2ede-b004-647ecf03e427"
                                            href="/testimonials" class="second-button w-inline-block">
                                            <div class="button-inner-2">
                                                <p class="button-title is-black">Clients say</p>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <div id="w-node-c4442ee1-5837-2ede-b004-647ecf03e42b-cf03e41b"
                                data-w-id="c4442ee1-5837-2ede-b004-647ecf03e42b" class="height-120-per">
                                <div class="_3d-block is-visible">
                                    <div class="image-animation-trigger _3d-for-image-visible">
                                        <div class="animation-color-bg"></div><img
                                            src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6255b4f7da0cd98b7ff456cd_pexels-max-vakhtbovych-6958122.jpg"
                                            loading="lazy" alt="Construction and renovation webflow template sample image"
                                            class="cover-image" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="blue-95-background"></div>
                </section>
            </main>
        </div>
   <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=625295532800cfe2dd8d3dd2" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
      <script src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/js/webflow.097fc263b.js" type="text/javascript"></script>
    </div>
</body>
{% endblock %}