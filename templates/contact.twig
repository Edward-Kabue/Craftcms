
{% extends "layout/contact.twig" %}


{% block content %}

<body>
    <div id="webpage" class="page-wrapper">
        <div class="global-styles w-embed"></div>
        {% set photo = entry.bgImage.one() %}
     
        <div class="second-navbar"><a href="#" class="aside-contact-button w-inline-block"><img
                    src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6258131bac44e22abdd9e8ce_email_white_24dp.svg"
                    loading="lazy" alt="" class="aside-icon" /></a><a href="#" class="scroll-to-button w-inline-block">
                <div class="aside-button-line"></div><img
                    src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/625319874a18755529409557_expand_more_white_24dp.svg"
                    loading="lazy" alt="" class="icon-scroll-to" />
            </a><a href="#" class="right-menu-button is-goes-up w-inline-block"><img
                    src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/62587db8e356a3457738948a_expand_less_white_24dp.svg"
                    loading="lazy" alt="" class="goes-up-icon" /></a></div>
        <main class="main">
            <section class="section page-hero-sec wf-section">
                <div class="hero-5-header">
                    <div class="container">
                        <h1 data-w-id="1a10983e-9719-6645-5a9a-de1442134c0a" style="opacity:0; color:white;align-items:center; "
                            class="heading in-contact-2">{{entry.heroText}}
                            </h1>
                    </div>
                    <div class="absolute-block">
                        <div class="hero-bg-color"></div>
                        <div class="hero-bg-gradient"></div>
                        <div class="_3d-block">
                            <div class="image-animation-trigger _3d-for-image">
                                <div class="animation-color-bg"></div><img
                                    src="{{photo.url}}"
                                    loading="lazy" sizes="100vw"
                                    {# srcset="https://assets.website-files.com/625295532800cfe2dd8d3dd2/626015735c032d1ded9e9a7b_pexels-mikael-blomkvist-8961401-p-500.jpeg 500w, https://assets.website-files.com/625295532800cfe2dd8d3dd2/626015735c032d1ded9e9a7b_pexels-mikael-blomkvist-8961401-p-800.jpeg 800w, https://assets.website-files.com/625295532800cfe2dd8d3dd2/626015735c032d1ded9e9a7b_pexels-mikael-blomkvist-8961401-p-1080.jpeg 1080w, https://assets.website-files.com/625295532800cfe2dd8d3dd2/626015735c032d1ded9e9a7b_pexels-mikael-blomkvist-8961401.jpg 1920w" #}
                                    alt="site_consult_steel"
                                    class="cover-image" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="hero-absolute-block">
                    <div class="hero-sticky-block">
                        <div class="w-dyn-list">
                            <div role="list" class="hero-socials-list in-hero-2 w-dyn-items">
                                <div role="listitem" class="w-dyn-item"><a href="/socials/youtube"
                                        class="social-link-block w-inline-block"><img
                                            src="https://assets.website-files.com/6252f3c395e36c6da2794a2c/62530e0b95e36cefd17a0041_twitter-logo-white.svg"
                                            loading="lazy" alt="" class="moving-icon" />
                                        <div style="border-color:red" class="moving-link-circle"></div>
                                    </a></div>
                                <div role="listitem" class="w-dyn-item"><a href="/socials/facebook"
                                        class="social-link-block w-inline-block"><img
                                            src="https://assets.website-files.com/6252f3c395e36c6da2794a2c/62530e1178c1b122e497e2a5_facebook-logo-white.svg"
                                            loading="lazy" alt="" class="moving-icon" />
                                        <div style="border-color:#1877f2" class="moving-link-circle"></div>
                                    </a></div>
                                <div role="listitem" class="w-dyn-item"><a href="/socials/instagram"
                                        class="social-link-block w-inline-block"><img
                                            src="https://assets.website-files.com/6252f3c395e36c6da2794a2c/62530e183660db0a309e3e2b_instagram-logo-white.svg"
                                            loading="lazy" alt="" class="moving-icon" />
                                        <div style="border-color:#833ab4" class="moving-link-circle"></div>
                                    </a></div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <div class="pattern-divider"></div>
            <section class="section contact-sec wf-section">
                <div class="container">
                    <div class="contact-2-inner-1">
                        <div class="contact-2-grid">
                            <div id="w-node-d05a6ffb-970a-b22f-b3fd-8894e8b9eb32-4ef2f445" class="contact-2-column-1">
                                <h2 class="heading is-contact-title-2">Contact</h2>
                                <div data-w-id="916ada64-0b5e-8b31-42f9-1de48b8d031d"
                                    class="contact-block-1 full-border">
                                 
                                    <p class="content-title-2 in-contact">Address</p>
                                    <p class="content-anim-trigger in-contact">{{entry.address}}</p>
                                    </p>
                                    <div style="-webkit-transform:translate3d(0, 0, 0) scale3d(1, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 0, 0) scale3d(1, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 0, 0) scale3d(1, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 0, 0) scale3d(1, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0)"
                                        class="border-anim-trigger"></div><a href="{{entry.mapLink}}" target="_blank"
                                        class="text-link in-contact">Open map</a>
                                </div>
                                <div data-w-id="d8814c73-0d74-e0e7-84da-a18a5332c4e6"
                                    class="contact-block-1 full-border is-last">
                                   
                                    <p class="content-title-2 in-contact">Contact</p><a href="mailto: {{entry.email}}"
                                        class="text-link in-contact is-email">{{entry.email}}</a><a href="tel: {{entry.phone}}"
                                        class="text-link in-contact is-phone">
                                        {{entry.phone}}
                                        </a>
                                    <div style="-webkit-transform:translate3d(0, 0, 0) scale3d(1, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 0, 0) scale3d(1, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 0, 0) scale3d(1, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 0, 0) scale3d(1, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0)"
                                        class="border-anim-trigger"></div>
                                </div>
                                <h2 class="heading is-contact-title-2">Team</h2>
                                {% for contact in entry.persons.all() %}
                                <div data-w-id="d7ee8872-a53c-2a96-bbc3-d5146859767f" class="contact-block-1">
                                    <p class="content-title-2 is-name">{{contact.contactPerson}}</p>
                                    <p data-w-id="d7ee8872-a53c-2a96-bbc3-d51468597682" style="opacity:0"
                                        class="person-role">{{contact.contactPosition}}</p><a href="mailto:{{contact.contactEmail}}" class="text-link in-contact">Email
                                        me</a><a href="tel:{{contact.contactPhone}}" class="text-link in-contact">Call me</a>
                                    <div style="-webkit-transform:translate3d(0, 0, 0) scale3d(1, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 0, 0) scale3d(1, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 0, 0) scale3d(1, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 0, 0) scale3d(1, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0)"
                                        class="border-anim-trigger"></div>
                                </div>
                                {% endfor %}
                             
                            </div>
                            <div id="w-node-_64cd2cd7-c438-38cd-385f-faad00276946-4ef2f445" class="contact-2-column-2">
                                <div id="w-node-_9a021312-ab24-2cd5-5415-994c6659baf1-4ef2f445"
                                    data-w-id="9a021312-ab24-2cd5-5415-994c6659baf1" class="form-block">
                                    <div class="form w-form">
                                        <form id="wf-form-Contact-form" name="wf-form-Contact-form"
                                            data-name="Contact form" method="get" class="form-inner">
                                            <div class="form-item"><label for="full-form-2-full-name"
                                                    class="form-label">Full Name</label><input type="text"
                                                    class="form-field w-input" maxlength="256" name="Full-Name"
                                                    data-name="Full Name" placeholder="John Boyd"
                                                    id="full-form-2-full-name" required="" /></div>
                                            <div class="form-item"><label for="full-form-2-email"
                                                    class="form-label">EMAIL</label><input type="email"
                                                    class="form-field w-input" maxlength="256" name="Email"
                                                    data-name="Email" placeholder="john.boyd@mail.com"
                                                    id="full-form-2-email" required="" /></div>
                                            <div class="form-item"><label for="full-form-2-phone"
                                                    class="form-label">Phone</label><input type="tel"
                                                    class="form-field w-input" maxlength="256" name="Phone"
                                                    data-name="Phone" placeholder="555 - 777 - 333"
                                                    id="full-form-2-phone" required="" /></div>
                                            <div class="form-item"><label for="full-form-2-address"
                                                    class="form-label">Address</label><input type="text"
                                                    class="form-field w-input" maxlength="256" name="Address"
                                                    data-name="Address" placeholder="Silber street, London"
                                                    id="full-form-2-address" /></div>
                                            <div class="form-item"><label for="full-form-2-property"
                                                    class="form-label">Property</label><input type="text"
                                                    class="form-field w-input" maxlength="256" name="Property"
                                                    data-name="Property" placeholder="Apartment"
                                                    id="full-form-2-property" /></div>
                                            <div class="form-item is-area-text"><label for="full-form-2-description"
                                                    class="form-label">Description</label><textarea
                                                    id="full-form-2-description" name="Description" maxlength="5000"
                                                    data-name="Description" placeholder=""
                                                    class="form-field text-area w-input"></textarea></div><label
                                                data-w-id="9a021312-ab24-2cd5-5415-994c6659bb0c" style="opacity:0"
                                                class="w-checkbox checkbox-field">
                                                <div
                                                    class="w-checkbox-input w-checkbox-input--inputType-custom checkbox">
                                                </div><input type="checkbox" name="Personal-Data" id="Personal-Data"
                                                    data-name="Personal Data" required=""
                                                    style="opacity:0;position:absolute;z-index:-1" /><span
                                                    class="checkbox-label w-form-label" for="Personal-Data">I want to
                                                    subscribe to the newsletter</span>
                                            </label><input type="submit" value="Send form" data-wait="Sending..."
                                                data-w-id="9a021312-ab24-2cd5-5415-994c6659bb10" style="opacity:0"
                                                class="submit-button w-button" />
                                        </form>
                                        <div class="form-success-message-2 w-form-done"><img
                                                src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/6258131c718f7ca900c76c0b_question_answer_white_24dp.svg"
                                                loading="lazy" alt="" class="form-success-icon" />
                                            <div class="form-success-title">Message sent!<br />Thank you.</div>
                                        </div>
                                        <div class="form-error-message w-form-fail">
                                            <p class="form-error-paragraph">An error has occurred somewhere and it is
                                                not possible to submit the form. Please try again later or <a href="#"
                                                    class="form-error-link">contact us via email.</a></p>
                                        </div>
                                    </div>
                                    <div class="form-pattern"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="blue-bg-absolute"></div>
            </section>
            <div class="pattern-divider"></div>


        </main>
    </div>
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=625295532800cfe2dd8d3dd2"
        type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
        crossorigin="anonymous"></script>
    <script src="https://assets.website-files.com/625295532800cfe2dd8d3dd2/js/webflow.097fc263b.js"
        type="text/javascript"></script>
    <!--[if lte IE 9]><script src="//cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif]-->
</body>

{% endblock %}